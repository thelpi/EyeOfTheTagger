<Window x:Class="EyeOfTheTagger.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EyeOfTheTagger"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <ProgressBar DockPanel.Dock="Bottom" Width="200" IsIndeterminate="False" Height="20" HorizontalAlignment="Left" Name="LoadingBar" Minimum="0" Maximum="100" />
        <ListBox DockPanel.Dock="Top" Visibility="Visible" Name="Console" />
        <ListBox DockPanel.Dock="Top" Visibility="Collapsed" Name="MainView" />
    </DockPanel>
</Window>
